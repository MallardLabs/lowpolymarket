# Staging Environment Configuration
ENVIRONMENT=staging
DEBUG=false

# Logging settings for staging
LOG_LEVEL=INFO
LOG_JSON_FORMAT=true
LOG_FILE_PATH=logs/staging.log

# Cache settings for staging
CACHE_DEFAULT_TTL=300
CACHE_PREDICTION_TTL=180
CACHE_MARKET_DATA_TTL=30

# Business logic settings for staging
BUSINESS_MAX_BET_AMOUNT=100000
BUSINESS_MIN_RESOLUTION_VOTES=2
BUSINESS_MAX_CONCURRENT_BETS=25

# Rate limiting settings for staging
RATE_LIMIT_USER_REQUESTS_PER_MINUTE=15
RATE_LIMIT_USER_BETS_PER_MINUTE=7
RATE_LIMIT_GUILD_REQUESTS_PER_MINUTE=150